<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <filter>
        <filter-name>Filter</filter-name>
        <filter-class>cn.com.benic.filter.Filter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>Filter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>CheckUserByNameController</servlet-name>
        <servlet-class>cn.com.benic.controller.CheckUserByNameController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckUserByNameController</servlet-name>
        <url-pattern>/CheckUserByName</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>cn.com.benic.controller.LoginController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RegistController</servlet-name>
        <servlet-class>cn.com.benic.controller.RegistController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistController</servlet-name>
        <url-pattern>/regist</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SellerEditController</servlet-name>
        <servlet-class>cn.com.benic.controller.SellerEditController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SellerEditController</servlet-name>
        <url-pattern>/sellerEdit</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SellerListController</servlet-name>
        <servlet-class>cn.com.benic.controller.SellerListController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SellerListController</servlet-name>
        <url-pattern>/sellerList</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SellerSelectByIdController</servlet-name>
        <servlet-class>cn.com.benic.controller.SellerSelectByIdController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SellerSelectByIdController</servlet-name>
        <url-pattern>/sellerSelectById</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SellerDeleteController</servlet-name>
        <servlet-class>cn.com.benic.controller.SellerDeleteController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SellerDeleteController</servlet-name>
        <url-pattern>/sellerDelete</url-pattern>
    </servlet-mapping>
</web-app>